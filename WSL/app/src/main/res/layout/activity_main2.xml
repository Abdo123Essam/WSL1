<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="310dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="0dp" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="74dp"
        android:layout_height="35dp"
        android:src="@drawable/image_four"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="398dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="60dp"
        android:layout_height="34dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="9dp"
        android:src="@drawable/image_five"
        app:layout_constraintBottom_toBottomOf="@+id/button_AddHome"
        app:layout_constraintTop_toTopOf="@+id/button_AddHome"
        tools:layout_editor_absoluteX="18dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="69dp"
        android:layout_height="37dp"
        android:layout_marginBottom="310dp"
        android:src="@drawable/image_six"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.19" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginEnd="212dp"
        android:layout_marginBottom="310dp"
        android:src="@drawable/Image__two"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button_Whereto"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.611" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginEnd="276dp"
        android:src="@drawable/image_three"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button_Whereto"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.643" />

    <Button
        android:id="@+id/button_book"
        android:layout_width="374dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:background="@color/cardview_shadow_start_color"
        android:text="From where ?"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/button_Whereto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="244dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/image_one"
        app:layout_constraintBottom_toBottomOf="@+id/button_book"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_book" />

    <Button
        android:id="@+id/button_Whereto"
        android:layout_width="374dp"
        android:layout_height="47dp"
        android:background="@color/cardview_shadow_start_color"
        android:text="Where to ?"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="227dp" />


    <Button
        android:id="@+id/button_addwork"
        android:layout_width="374dp"
        android:layout_height="47dp"
        android:background="@color/cardview_shadow_start_color"
        android:text="Add Work "
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="386dp" />

    <Button
        android:id="@+id/button_SavePlace"
        android:layout_width="374dp"
        android:layout_height="47dp"
        android:background="@color/cardview_shadow_start_color"
        android:text="Save Place "
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="445dp" />

    <Button
        android:id="@+id/button_AddHome"
        android:layout_width="374dp"
        android:layout_height="47dp"
        android:background="@color/cardview_shadow_start_color"
        android:text="Add Home"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="318dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="48dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_Whereto"
        app:layout_constraintTop_toTopOf="@+id/button_Whereto"
        app:srcCompat="@drawable/image_two"
        tools:layout_editor_absoluteX="18dp" />


</androidx.constraintlayout.widget.ConstraintLayout>